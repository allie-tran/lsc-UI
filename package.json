        "@types/react-redux": "^7.1.33",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@welldone-software/why-did-you-render": "^8.0.1",
        "antd": "^4.18.8",
        "autosuggest-highlight": "^3.2.1",
        "axios": "^0.19.2",
        "leaflet": "^1.6.0",
        "leaflet-area-select": "^1.0.3",
        "leaflet-polylinedecorator": "^1.6.0",
        "leaflet.markercluster": "^1.4.1",
        "lodash.isequal": "^4.5.0",
        "prettier": "^3.2.5",
        "react": "^17.0.0",
        "react-app-rewired": "^2.1.5",
        "react-chrono": "^1.14.0",
        "react-dom": "^17.0.0",
        "react-lazy-load": "^3.1.13",
        "react-lazy-load-image-component": "^1.5.6",
        "react-lazyload": "^3.2.0",
        "react-loading": "^2.0.3",
        "react-onclickoutside": "^6.9.0",
        "react-promise-tracker": "^2.1.0",
        "react-redux": "^7.2.0",
        "react-scripts": "^3.4.0",
        "react-sticky-mouse-tooltip": "0.0.1",
        "react-vertical-timeline-component": "^3.5.2",
        "react-window": "^1.8.5",
        "redux": "^4.0.5",
        "redux-api-middleware": "^3.2.0",
        "redux-axios-middleware": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "typeface-roboto": "0.0.75",
        "typescript": "^5.4.3",
        "use-sound": "^4.0.1",
        "yarn": "^1.22.22"
    },
    "scripts": {
        "start": "PORT=3000 react-scripts --openssl-legacy-provider start",
        "build": "GENERATE_SOURCEMAP=false react-scripts --openssl-legacy-provider build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "tsconfig": {
        "compilerOptions": {
            "target": "es5",
            "lib": [
                "dom",
                "dom.iterable",
                "esnext"
            ],
            "allowJs": true,
            "skipLibCheck": true,
            "esModuleInterop": true,
            "allowSyntheticDefaultImports": true,
            "strict": true,
            "forceConsistentCasingInFileNames": true,
            "module": "esnext",
            "moduleResolution": "node",
            "resolveJsonModule": true,
            "isolatedModules": true,
            "noEmit": true,
            "jsx": "react-jsx",
            "baseUrl": "./src",
            "paths": {
                "*": [
                    "src/*"
                ],
                "@components/*": [
                    "src/components/*"
                ],
                "@application/*": [
                    "src/application/*"
                ],
                "@assets/*": [
                    "src/assets/*"
                ]
            }
        },
        "include": [
            "src"
        ]
    }
}
